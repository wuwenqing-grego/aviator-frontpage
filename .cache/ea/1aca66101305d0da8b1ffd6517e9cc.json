k', () => {\n            open(item.url)\n        })\n        $site.on('click', '.delete', (e) => {\n            e.stopPropagation()\n            if (window.confirm('是否确认删除？')) {\n                sites.splice(index, 1)\n                saveToCache()\n                render()\n            }\n        })\n    })\n}\n\nrender()\n\n$('.addButton').on('click', () => {\n    let url = window.prompt('请输入新标签页的网址：')\n    if (url === '') {\n        window.alert('网址不可为空！')\n    } else if (url) {\n        if (!url.includes('http')) {\n            url = 'https://' + url\n        }\n        console.log(url)\n    \n        sites.push({logo: removePrefix(url)[0].toUpperCase(), url: url})\n        saveToCache()\n        render()\n    }\n})"},"lineCount":null}},"error":null,"hash":"c2342b129bf86b10bba7c50b873ad34d","cacheData":{"env":{}}}